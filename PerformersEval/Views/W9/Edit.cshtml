@model PerformersEval.Models.SubW9

@{
    ViewBag.Title = "Modify Sub W-9";
}

<div id="inv_container" style="position: relative" >
<div style="position: absolute; top:5px ;right:5px; border:solid; border-width: 1px;"><img src="~/Images/tin.png" width="100" height="400"></div>
    
@using (Html.BeginForm("Edit", "W9", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(false)

@Html.HiddenFor(model => model.SubW9ID)
@Html.HiddenFor(model => model.TIN)
@Html.HiddenFor(model => model.SSN1)
@Html.HiddenFor(model => model.SSN2)
@Html.HiddenFor(model => model.SSN3)
@Html.HiddenFor(model => model.SSN4)
@Html.HiddenFor(model => model.SSN5)
@Html.HiddenFor(model => model.SSN6)
@Html.HiddenFor(model => model.SSN7)
@Html.HiddenFor(model => model.SSN8)
@Html.HiddenFor(model => model.SSN9)

@Html.HiddenFor(model => model.EIN1)
@Html.HiddenFor(model => model.EIN2)
@Html.HiddenFor(model => model.EIN3)
@Html.HiddenFor(model => model.EIN4)
@Html.HiddenFor(model => model.EIN5)
@Html.HiddenFor(model => model.EIN6)
@Html.HiddenFor(model => model.EIN7)
@Html.HiddenFor(model => model.EIN8)
@Html.HiddenFor(model => model.EIN9)
  
<div>1110 - W9</div>
<div>Rev 10/04</div>
<div style="text-align:center; font-size:1.25em"><strong>COUNTY OF ALAMEDA</strong></div>
<div style="text-align:center; font-size:1.25em">1221 OAK STREET, ROOM 249, OAKLAND, CA 94612</div>
<div style="text-align:center; font-size:1.25em"><strong>Substitute IRS Form W-9</strong></div>
<div style="text-align:center; font-size:1.25em">Request for Taxpayer Identification Number and Certification</div>
    <br />    <br />

<div style="text-align:left"><i>The purpose of this form is to obtain or verify the accuracy of information regarding Alameda County's payees.</i></div>
<div style="text-align:left"><i>ALL, payees must have an accurate W-9 on file in the Auditor-Controller's office in order to be paid.</i></div>
<div style="text-align:left"><i>If you fail to furnish your correct TIN, you could be subject to a penalty.</i></div>
<div style="text-align:left"><b><i>The form must be <u>completed,</u> even if the information shown at the bottom of the form is accurate.</i></b></div>
<br />
<div><b>Please print or type.</b> Do not send to IRS. Return to Alameda County in the envelope provided.</div>

<table class="thinborder"> 
    <tr class="thinbordertop">
        <td class="inputthinborder">Name on record with IRS or Social Security Administration:</td>
    </tr>
    <tr class="inputthinborder">
        <td class="inputthinborder"><span>@Html.TextBoxFor(m => m.Name, htmlAttributes: new { style="width:99%" })@Html.ValidationMessageFor(m => m.Name)</span></td>
    </tr>

    <tr class="thinbordertop">
        <td class="inputthinborder">All DBA(s) <u>or</u> Invoice Name(s) (If different from the above name) - use attachments if neccessary:</td>
    </tr>
    <tr class="thinborder">
        <td class="inputthinborder"><span>@Html.TextBoxFor(m => m.OtherNames, htmlAttributes: new { style="width:99%" })</span></td>
    </tr>

    <tr class="thinbordertop">
        <td class="inputthinborder">Address for Correspondence or 1099 (we will take the remittance address if different, from the invoice)</td>
    </tr>
    <tr class="thinborder">
        <td class="inputthinborder"><span>@Html.TextBoxFor(m => m.Addr, htmlAttributes: new { style="width:99%" })</span></td>
    </tr>
</table>


<table class="thinborder">
    <colgroup>
        <col style="width:30%;"/>
        <col style="width:40%;"/>
        <col style="width:30%;"/>
    </colgroup>

    <tr>
        <th colspan="3" style="text-align:center">TAXPAYER IDENTIFICATION NUMBER (TIN)</th>
    </tr>
    <tr>
         <td colspan="3" class="leftpadding">You may enter <u>only</u> one TIN and it must be the type of TIN (SSN or EIN) that is appropriate to your type of entity. If you do not have a TIN, or for further<br />
            information, see the instruction on the second page.</td>
   </tr>

    <tr>
         <th style="text-align: right; ">SOCIAL SECURITY NUMBER:</th>
        <td style="text-align: left;"  class="leftpadding">
            @Html.TextBoxFor(m => m.SSN1, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN1)
            @Html.TextBoxFor(m => m.SSN2, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN2)
            @Html.TextBoxFor(m => m.SSN3, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN3)&nbsp;-&nbsp;
            @Html.TextBoxFor(m => m.SSN4, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN4)
            @Html.TextBoxFor(m => m.SSN5, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN5)&nbsp;-&nbsp;
            @Html.TextBoxFor(m => m.SSN6, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN6)
            @Html.TextBoxFor(m => m.SSN7, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN7)
            @Html.TextBoxFor(m => m.SSN8, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN8)
            @Html.TextBoxFor(m => m.SSN9, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.SSN9)
        </td>
        <td rowspan="3"><div style="text-align:center; border:solid;border-width:1px; background-color:lightgray; margin-left:60px; margin-right:60px;">ONLY <u>ONE</u><br />NUMBER WILL<br />BE ACCEPTED</div></td>
    </tr>

    <tr>
         <th style="text-align: center; ">OR</th>
        <th style="text-align: left;" > </th>
    </tr>

    <tr>
         <th style="text-align: right; ">EMPLOYER ID NUMBER:</th>
        <th style="text-align: left;" >
            @Html.TextBoxFor(m => m.EIN1, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN1)
            @Html.TextBoxFor(m => m.EIN2, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN2)&nbsp;-&nbsp;
            @Html.TextBoxFor(m => m.EIN3, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN3)
            @Html.TextBoxFor(m => m.EIN4, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN4)
            @Html.TextBoxFor(m => m.EIN5, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN5)
            @Html.TextBoxFor(m => m.EIN6, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN6)
            @Html.TextBoxFor(m => m.EIN7, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN7)
            @Html.TextBoxFor(m => m.EIN8, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN8)
            @Html.TextBoxFor(m => m.EIN9, htmlAttributes: new {disabled = "disabled", style="width:1em", maxlength="1"})@Html.ValidationMessageFor(m => m.EIN9)
        </th>
    </tr>

    <tr>
         <td colspan="3" class="leftpadding">TIN <u>MUST</u> BE ENTERED REGARDLESS OF TYPE OF ENTITY (IE, NON-PROFIT, RETAIL CORPORATION, ETC)</td>
    </tr>
</table>


<table class="thinborder">
    <colgroup>
            <col style="width:50%;" /> 
            <col style="width:50%;" /> 
    </colgroup>

    <th colspan="2" style="text-align:center;"><b>Type of Entity (Please check only one)</b></th>

    <tr>
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.Individual)&nbsp;&nbsp;INDIVIDUAL</td>
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.SoleProprietor)&nbsp;&nbsp;SOLE PROPRIETOR</td>
    </tr>
    <tr style="border:solid; border-width:1px;">
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.Partnership)&nbsp;&nbsp;PARTNERSHIP</td>
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.Corporation)&nbsp;&nbsp;CORPORATION</td>
    </tr>
    <tr style="border:solid; border-width:1px;">
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.TaxExempted)&nbsp;&nbsp;TAX-EXEMPT ORGANIZATION UNDER SECTION 501(C)</td>
        <td class="checkboxwithborder" >@Html.CheckBoxFor(m => m.GovOrTrust)&nbsp;&nbsp;GOVERMENT OR TRUST (SPECIFY)</td>
    </tr>
</table>

<div style="border-style:solid; border-width:1px;">
<table class="noborder">
    <colgroup>
        <col style="width:20%;" /> 
        <col style="width:20%;" /> 
        <col style="width:20%;" /> 
        <col style="width:40%;" /> 
    </colgroup>

    <tr>
        <td colspan="4" class="thinborderbottom" style="padding-left: 10px;">CHECK THE BOXES THAT APPLY TO <b>Alameda County's</b> PAYMENT TO YOU:
        </td>
    </tr>

    <tr>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.GoodsOnly)&nbsp;&nbsp;GOOD ONLY</td>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.GoodsAndServices)&nbsp;&nbsp;GOODS AND SERVICES</td>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.RentsLeases)&nbsp;&nbsp;RENTS / LEASES</td>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.RentsLeasesAsAgent)&nbsp;&nbsp;RENTS / LEASES PAID TO YOU AS THE AGENT</td>
    </tr>
    <tr>
        <td style="padding-left: 20px;" colspan="2">@Html.CheckBoxFor(m => m.MedicalService)&nbsp;&nbsp;MEDICAL AND HEALTHCARE SERVICES</td>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.LegalService)&nbsp;&nbsp;LEGAL_SERVICES</td>
        <td style="padding-left: 20px;">@Html.CheckBoxFor(m => m.Settlement)&nbsp;&nbsp;SETTLEMENT</td>
    </tr>
    <tr>
        <td style="padding-left: 20px;" colspan="4" >@Html.CheckBoxFor(m => m.OtherServices)&nbsp;&nbsp;OTHER SERVICES - DESCRIBE:<span>@Html.TextBoxFor(m => m.OtherServices_Desc)</span></td>
    </tr>
</table>
</div>
      
<div style="border-style:solid; border-width:1px;">
<table class="noborder">
      <tr>
        <td class="thinborderbottom" style="padding-left: 20px" colspan="4" >@Html.CheckBoxFor(m => m.ExemptFromBackupWithHolding)&nbsp;&nbsp;CHECK THIS BOX if you are exempt from backup withholding. Entities exempt from backup withholding are listed on the second page.</td>
    </tr>

    <tr>
        <td style="padding-left: 10px;"  colspan="4" >Certification - Under penalties of perjury, I certify that:<br />
            (1) The number shown on this form is my correct taxpayer information number (or I am waiting for a number to be issued to me).<br />
            (2) I am not subject to backup withholding because (a) I am exempt from backup withholding, or (b) I have not been notified by the IRS that I am subject to backup withholding as a result of failure to report all interest of dividends, or (c) the IRS has notified me that I am no longer subject to backup withholding. (<a href="~/Images/w9instructions.pdf" target="_blank"><b>For instructions, see the second page.</b></a>)
        </td>
    </tr>
</table>
</div>

<table class="noborder">
    <colgroup>
        <col style="width:50%;" /> 
        <col style="width:50%;" /> 
    </colgroup>

    <tr>
        <td style="vertical-align: top;">
            The purpose of this form is to verify the accuracy of the information we currently have on our records and to obtain your certification for our files. PLEASE DON’T MARK THE LABEL BELOW; WE USE IT TO VERIFY THE ACCURACY OF OUR CURRENT INFORMATION.
       </td>
       <td style="vertical-align: top;">
                <table  style="border-style:solid; border-width:1px;">   <!-- class="noborder"> -->
                    <colgroup>
                        <col style="width:15%;" /> 
                        <col style="width:45%;"/> 
                        <col style="width:10%;" /> 
                        <col style="width:30%;" /> 
                    </colgroup>

                   <tr>
                       <td colspan="4">Please sign here (required).<td />
                    </tr>
                    <tr>
                        <td colspan="4">Check if this signature applies to Certification (1) only)&nbsp;&nbsp;@Html.CheckBoxFor(m => m.SigAppliesToCertification)<td />
                    </tr>
                    <tr><td><br /></td></tr>

                   <tr>
                        <td>Signature</td>
                        <td colspan="3"><span>@Html.TextBoxFor(m => m.SignedSignature)@Html.ValidationMessageFor(m => m.SignedSignature)</span><td />
                    </tr>
                    <tr>
                        <td>Print name</td>
                        <td colspan="3"><span>@Html.TextBoxFor(m => m.SignedPrintName)@Html.ValidationMessageFor(m => m.SignedPrintName)</span><td />
                    </tr>
                   <tr>
                        <td>Title</td>
                        <td><span>@Html.TextBoxFor(m => m.SignedTitle)</span></td>
                        <td style="text-align:right;">Date</td>
                        <td><span>@Html.TextBoxFor(m => m.SignedDate, "{0:MM/dd/yyyy}", new { @class = "date", style = "width:5em" })@Html.ValidationMessageFor(m => m.SignedDate)</span></td>
                    </tr>
                    <tr>
                        <td>Phone Number</td>
                        <td colspan="3"><span>@Html.TextBoxFor(m => m.SignedPhone, htmlAttributes: new { style="width:15em" })@Html.ValidationMessageFor(m => m.SignedPhone)</span><td />
                    </tr>
                    <tr>
                        <td>Fax Number</td>
                        <td colspan="3"><span>@Html.TextBoxFor(m => m.SignedFAX, htmlAttributes: new { style="width:15em" })@Html.ValidationMessageFor(m => m.SignedFAX)</span><td />
                    </tr>
                   <tr>
                        <td>e-mail address</td>
                        <td colspan="3"><span>@Html.TextBoxFor(m => m.SignedEmail)</span><td />
                    </tr>
               </table>
      </td>
    </tr>
</table>
 
<p>
    <input type="submit" value="Save" />
</p>
           
}



@section Scripts {
@Scripts.Render("~/bundles/jqueryval")

@Scripts.Render("~/bundles/jqueryui")
@Styles.Render("~/Content/themes/base/css")

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $(".date").datepicker();
    });
</script>
}